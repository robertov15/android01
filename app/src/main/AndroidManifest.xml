<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roberto.fortezopv" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".VentaListActivity"
            android:label="@string/titulo_ventana" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VentaDetailActivity"
            android:label="@string/title_venta_detail"
            android:parentActivityName=".VentaListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".VentaListActivity" />
        </activity>
        <activity
            android:name=".VentaDetalles"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_venta_detalles"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".formasPago"
            android:label="@string/title_activity_formas_pago" >
        </activity>
    </application>

</manifest>
